#!/usr/bin/env python3
import sympy
import sys

def rsa_factorize(filename):
    # Read numbers from file
    with open(filename, 'r') as file:
        numbers = [int(line.strip()) for line in file]

    # Factorize prime numbers and print results
    for num in numbers:
        p, q = sympy.factorint(num).as_ordered_factors()
        print(f"{num}={p}*{q}")

if __name__ == "__main__":
    # Check for correct usage
    if len(sys.argv) != 2:
        print("Usage: ./rsa.py <filename>")
        sys.exit(1)

    filename = sys.argv[1]
    rsa_factorize(filename)
