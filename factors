#!/usr/bin/env python3
import sympy
import sys

def factorize_numbers(filename):
    # Read numbers from file
    with open(filename, 'r') as file:
        numbers = [int(line.strip()) for line in file]

    # Factorize and print results
    for num in numbers:
        factors = list(sympy.factorint(num).items())
        factorization = '*'.join(f'{p}^{exp}' for p, exp in factors)
        print(f"{num}={factorization}")

if __name__ == "__main__":
    # Check for correct usage
    if len(sys.argv) != 2:
        print("Usage: ./factors.py <filename>")
        sys.exit(1)

    filename = sys.argv[1]
    factorize_numbers(filename)
